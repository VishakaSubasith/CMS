<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UpdateTimetable"
        tools:layout_editor_absoluteY="81dp">

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="92dp"
            android:layout_height="65dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/ic_timetable_frog" />

        <Button
            android:id="@+id/button16"
            android:layout_width="130dp"
            android:layout_height="50dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="12dp"
            android:background="@color/colorPrimary"
            android:text="@string/delete"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText31" />

        <Button
            android:id="@+id/button17"
            android:layout_width="153dp"
            android:layout_height="52dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="36dp"
            android:background="@color/colorPrimary"
            android:text="@string/update"
            app:layout_constraintBaseline_toBaselineOf="@+id/button16"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/button16" />

        <TextView
            android:id="@+id/textView43"
            android:layout_width="92dp"
            android:layout_height="36dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="@string/notice"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView41" />

        <Spinner
            android:id="@+id/spinner5"
            android:layout_width="150dp"
            android:layout_height="51dp"
            android:layout_marginStart="40dp"
            android:background="#00E6D7D7"
            android:entries="@array/numbers"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView44"
            app:layout_constraintStart_toEndOf="@+id/textView44" />

        <TextView
            android:id="@+id/textView44"
            android:layout_width="58dp"
            android:layout_height="44dp"
            android:layout_marginStart="40dp"
            android:text="@string/type"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView43"
            app:layout_constraintStart_toEndOf="@+id/textView43" />

        <EditText
            android:id="@+id/editText28"
            android:layout_width="0dp"
            android:layout_height="49dp"
            android:layout_marginStart="8dp"
            android:background="#FFFFF1CC"
            android:ems="10"
            android:inputType="text|time"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView40"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView40" />

        <TextView
            android:id="@+id/textView40"
            android:layout_width="88dp"
            android:layout_height="39dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="8dp"
            android:text="@string/date"
            android:textSize="24sp"
            app:layout_constraintEnd_toStartOf="@+id/lnametxt"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView39" />

        <TextView
            android:id="@+id/textView41"
            android:layout_width="107dp"
            android:layout_height="43dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="@string/duration"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView40" />

        <EditText
            android:id="@+id/editText29"
            android:layout_width="78dp"
            android:layout_height="35dp"
            android:layout_marginStart="12dp"
            android:background="#FFFFF1CC"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView41"
            app:layout_constraintStart_toEndOf="@+id/textView41" />

        <EditText
            android:id="@+id/editText30"
            android:layout_width="92dp"
            android:layout_height="41dp"
            android:layout_marginEnd="16dp"
            android:background="#FFFFF1CC"
            android:ems="10"
            android:inputType="datetime"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView42"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView42"
            android:layout_width="48dp"
            android:layout_height="35dp"
            android:layout_marginEnd="8dp"
            android:text="@string/tocha"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/editText29"
            app:layout_constraintEnd_toStartOf="@+id/editText30" />

        <TextView
            android:id="@+id/textView39"
            android:layout_width="90dp"
            android:layout_height="38dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="44dp"
            android:text="@string/venue"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView38" />

        <EditText
            android:id="@+id/editText27"
            android:layout_width="0dp"
            android:layout_height="53dp"
            android:layout_marginStart="8dp"
            android:background="#FFFFF1CC"
            android:ems="10"
            android:inputType="text"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView39"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView39" />

        <EditText
            android:id="@+id/editText26"
            android:layout_width="0dp"
            android:layout_height="49dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="4dp"
            android:background="#FFFFF1CC"
            android:ems="10"
            android:inputType="text"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView38"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.918"
            app:layout_constraintStart_toEndOf="@+id/textView38" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="90dp"
            android:layout_height="34dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="40dp"
            android:text="@string/batch"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView37" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="90dp"
            android:layout_height="38dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="@string/subject"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView29" />

        <EditText
            android:id="@+id/editText25"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginStart="8dp"
            android:background="#FFFFF1CC"
            android:ems="10"
            android:inputType="text"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView37"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView37" />

        <TextView
            android:textAlignment="center"
            android:id="@+id/textView29"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:text="@string/updateshedule"
            android:textColor="#131313"
            android:textSize="28sp"
            app:layout_constraintEnd_toStartOf="@+id/imageView14"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editText31"
            android:layout_width="0dp"
            android:layout_height="122dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="28dp"
            android:background="#FFFFF1CC"
            android:ems="10"
            android:inputType="text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.75"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView43" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>